  "homepage": "https://github.com/ibarapascal/netsearch#readme",
  "keywords": [
    "react",
    "typescript",
    "vite"
  ],
  "main": "src/index.tsx",
  "prettier": "@g123jp/prettier-config-recommended",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibarapascal/netsearch.git"
  },
  "scripts": {
    "arkit:components": "npx arkit -d src -e \"src/types\" -o ./docs-components.png",
    "build": "vite build --mode ${RUN_ENV-production}",
    "dev": "vite --host",
    "fix": "yarn fix:ts && yarn fix:css",
    "fix:css": "stylelint --fix \"./src/**/*.scss\"",
    "fix:ts": "eslint --fix \"./**/*.ts\" \"./**/*.tsx\"",
    "fixpack": "npx fixpack",
    "lint": "tsc --noEmit && yarn lint:ts && yarn lint:css",
    "lint:css": "stylelint --cache \"./src/**/*.scss\"",
    "lint:ts": "tsc --noEmit && eslint \"./**/*.ts\" \"./**/*.tsx\""
  }
