{
  "name": "netsearch",
  "description": "Search github content with debounce, throttle, and pagination",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/ibarapascal/netsearch/issues"
  },
  "dependencies": {
    "antd": "^5.0.7",
    "classnames": "^2.3.2",
