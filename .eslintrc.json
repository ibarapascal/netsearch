{
  "plugins": [
    "react",
    "@typescript-eslint",
    "prettier",
    "import-helpers"
  ],
  "extends": [
    "@g123jp/eslint-config-react"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
